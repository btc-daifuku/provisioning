[
  {
    "name": "OpenAM",
    "image": "734729694574.dkr.ecr.ap-northeast-1.amazonaws.com/openam:setting",
    "memory": 1024,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 8080
      }
    ],
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "awslogs-${app_name}",
            "awslogs-region": "${aws_region}",
            "awslogs-stream-prefix": "awslogs-${app_name}-openam"
        }
    },
    "environment": [
        {
            "name": "CATALINA_OPTS",
            "value": "-server -Xmx1g -Xms1g"
        }
    ]
  }
]
